 <table class="table table-striped  table-hover  table-bordered  table-condensed">
    <thead>
      <tr>
        
        <% @columns.each do | column| %>
        <th><%=column.name.to_s.encode('utf-8') %>  <%=column.description.to_s.encode('utf-8') %></th>
        <% end %> 
      </tr>
    </thead>
    <tbody>
    
    </tbody>
  </table>
